      <PERIOD_V_LIMIT_1>0.0625</PERIOD_V_LIMIT_1>
      <V_LIMIT_2>50</V_LIMIT_2>
      <!-- <PERIOD_V_LIMIT_2>0.05</PERIOD_V_LIMIT_2> -->
