      <PERIOD_V_LIMIT_4>0.033</PERIOD_V_LIMIT_4>
      <PlACEBALL_SPEED>150</PlACEBALL_SPEED>
      <PlACEBALL_ROT_SPEED>3</PlACEBALL_ROT_SPEED>
      <PlACEBALL_ACCELERATION>50</PlACEBALL_ACCELERATION>
      <PlACEBALL_DECELERATION>50</PlACEBALL_DECELERATION>
      <PlACEBALL_ROT_ACCELERATION>3</PlACEBALL_ROT_ACCELERATION>
      <PlACEBALL_CLOSE_DISTANCE>100</PlACEBALL_CLOSE_DISTANCE>
