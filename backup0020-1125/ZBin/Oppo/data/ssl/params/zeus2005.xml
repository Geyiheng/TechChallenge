<?xml version="1.0" encoding="UTF-8" ?>
<zeus2005 xmlns="http://tempuri.org/zeus2005.xsd">
    <PlaceBall>
        <VERBOSE>0</VERBOSE>
        <WAIT_BUFFER>60</WAIT_BUFFER>
        <GETBALLMAXCNT>10</GETBALLMAXCNT>
    </PlaceBall>
  
  <Debug>
    <DRAW_RRT>0</DRAW_RRT>
    <DRAW_TRAJ>1</DRAW_TRAJ>
    <DRAW_OBS>1</DRAW_OBS>
    <DRAW_BALLPLACE_AREA>1</DRAW_BALLPLACE_AREA>
  </Debug>

  <Capability>
    <RECORD_COMMAND>1</RECORD_COMMAND>
    <SLOW_FACTOR>0.65</SLOW_FACTOR>
    <DONT_SEND>0</DONT_SEND><!-- only open on debug --> 

    <!-- 守门员单独控制平动参数 -->
    <MAX_TRANSLATION_SPEED_GOALIE>300</MAX_TRANSLATION_SPEED_GOALIE>
    <MAX_TRANSLATION_ACC_GOALIE>300</MAX_TRANSLATION_ACC_GOALIE>
    <MAX_TRANSLATION_DEC_GOALIE>300</MAX_TRANSLATION_DEC_GOALIE>
    <MAX_ROTATION_ACC_GOALIE>5</MAX_ROTATION_ACC_GOALIE>
    <MAX_ROTATION_SPEED_GOALIE>10</MAX_ROTATION_SPEED_GOALIE>

    <!-- 后卫单独控制平动参数 -->
    <MAX_TRANSLATION_SPEED_BACK>300</MAX_TRANSLATION_SPEED_BACK>
    <MAX_TRANSLATION_ACC_BACK>300</MAX_TRANSLATION_ACC_BACK>
    <MAX_TRANSLATION_DEC_BACK>300</MAX_TRANSLATION_DEC_BACK>
    <MAX_ROTATION_ACC_BACK>5</MAX_ROTATION_ACC_BACK>
    <MAX_ROTATION_SPEED_BACK>10</MAX_ROTATION_SPEED_BACK>

    <!-- 其他车的平动参数 -->
    <MAX_TRANSLATION_SPEED>300</MAX_TRANSLATION_SPEED><!-- 450 -->
    <MAX_TRANSLATION_ACC>100</MAX_TRANSLATION_ACC>
    <MAX_TRANSLATION_DEC>300</MAX_TRANSLATION_DEC>
    <MAX_ROTATION_ACC>5</MAX_ROTATION_ACC>
    <MAX_ROTATION_SPEED>10</MAX_ROTATION_SPEED>

    <TRANSLATION_ACC_LIMIT>500</TRANSLATION_ACC_LIMIT>
    <TRANSLATION_SPEED_LIMIT>300</TRANSLATION_SPEED_LIMIT>
    <TRANSLATION_ROTATE_ACC_LIMIT>5</TRANSLATION_ROTATE_ACC_LIMIT>
  </Capability>

  <BSpline>
    <DRAW_RRT>0</DRAW_RRT><!-- only open on debug -->
    <DRAW_TRAJ>1</DRAW_TRAJ>
    <BUFFER>10</BUFFER>
    <INFLAT_RATIO>1.0</INFLAT_RATIO>
    <LOWER_BND_UDOT>2.0</LOWER_BND_UDOT>
    <MAX_SEARCH_ITERATION>2</MAX_SEARCH_ITERATION>
    <COUNTPLAN_THRESHOLD>90</COUNTPLAN_THRESHOLD>

    <!-- 守门员单独控制平动参数 -->
    <MAX_TRANSLATION_SPEED_GOALIE>200</MAX_TRANSLATION_SPEED_GOALIE>
    <MAX_TRANSLATION_ACC_GOALIE>200</MAX_TRANSLATION_ACC_GOALIE>
    <MAX_TRANSLATION_DEC_GOALIE>200</MAX_TRANSLATION_DEC_GOALIE>
    <MAX_ROTATION_ACC_GOALIE>15</MAX_ROTATION_ACC_GOALIE>
    <MAX_ROTATION_SPEED_GOALIE>15</MAX_ROTATION_SPEED_GOALIE>

    <!-- 后卫单独控制平动参数 -->
    <MAX_TRANSLATION_SPEED_BACK>100</MAX_TRANSLATION_SPEED_BACK>
    <MAX_TRANSLATION_ACC_BACK>100</MAX_TRANSLATION_ACC_BACK>
    <MAX_TRANSLATION_DEC_BACK>100</MAX_TRANSLATION_DEC_BACK>
    <MAX_ROTATION_ACC_BACK>15</MAX_ROTATION_ACC_BACK>
    <MAX_ROTATION_SPEED_BACK>5</MAX_ROTATION_SPEED_BACK>

    <!-- 其他车的平动参数 -->
    <MAX_TRANSLATION_SPEED>200</MAX_TRANSLATION_SPEED><!-- 300 -->
    <MAX_TRANSLATION_ACC>300</MAX_TRANSLATION_ACC>
    <MAX_TRANSLATION_DEC>300</MAX_TRANSLATION_DEC>
    <MAX_ROTATION_SPEED>5</MAX_ROTATION_SPEED>
    <MAX_ROTATION_ACC>10</MAX_ROTATION_ACC>

    <TRANSLATION_ACC_LIMIT>200</TRANSLATION_ACC_LIMIT>
    <TRANSLATION_SPEED_LIMIT>200</TRANSLATION_SPEED_LIMIT>
    <TRANSLATION_ROTATE_ACC_LIMIT>5</TRANSLATION_ROTATE_ACC_LIMIT>
    
  </BSpline>

  <General>
    <PENALTY_FIGHT>1</PENALTY_FIGHT>
  </General>
  <Defence>
    <DEFENCE_DEBUG_MODE>1</DEFENCE_DEBUG_MODE>
    <ATTACK_DEF_MODE>1</ATTACK_DEF_MODE>
    <GOALIE_EVALUATE>0</GOALIE_EVALUATE>
    <DENY_BALL_MODE>1</DENY_BALL_MODE>
    <BACK_LINE_MODE>1</BACK_LINE_MODE>
    <SPECIAL_AREA_BACK_LINE_MODE>0</SPECIAL_AREA_BACK_LINE_MODE>

    <!-- 设置边后卫是否会盯人，1是会，0是不会 -->
    <SIDEBACK_MARKING_MODE>1</SIDEBACK_MARKING_MODE>
    
    <!-- 0是友好模式，1是普通模式，2是暴力模式 -->
    <MARKING_MODE>0</MARKING_MODE>

    <!-- 守门员上前补位模式 0为不补位 -->
    <GOALIE_FILL_IN_MODE>0</GOALIE_FILL_IN_MODE>
  </Defence>
  <CMatchState>
    <BAYESLIST>BayesParams1</BAYESLIST>
    <BAYESPARAM>Skuba_Attack</BAYESPARAM>
  </CMatchState>

  <ADVANCE> <!-- advance相关参数 -->
    <HEAD_LIMIT>15</HEAD_LIMIT>
    <GETBALL_BIAS>-3</GETBALL_BIAS>
    <IS_DRIBBLE>1</IS_DRIBBLE>
    <SHOOT_PRECISION>5</SHOOT_PRECISION>
    <!-- int  射门精度，（越大越不容易射），射门偏差角度为PI/SHOOT_PRECISION -->
    <ADV_CPASSPOWER>200</ADV_CPASSPOWER>
    <!-- int  挑传力度 -->
    <ADV_FPASSPOWER>380</ADV_FPASSPOWER>
    <!-- int 平传力度 -->
    <RELIEF_POWER>290</RELIEF_POWER>
    <!-- int 解围挑传力度 -->
    <BACK_POWER>400</BACK_POWER>
    <RELIEF_POWER_V3>620</RELIEF_POWER_V3>
    <BACK_POWER_V3>160</BACK_POWER_V3>
    <TEND_TO_SHOOT_FLAG>23</TEND_TO_SHOOT_FLAG>
    <SHOOT_BLOCKED_RANGE>1.4</SHOOT_BLOCKED_RANGE>
    <!-- double 射门时判定为会被拦截的范围上限，越大越容易判定为被拦截 -->
    <PASS_BLOCKED_RANGE>5</PASS_BLOCKED_RANGE>
    <!-- double 传球时判定为会被拦截的范围上限，越大越容易判定为被拦截 -->
    <RELIEF_BIAS>180</RELIEF_BIAS>
    <!-- double 传球时判定为会被拦截的范围上限，越大越容易判定为被拦截 -->

    <CHIP_ANGLE>15</CHIP_ANGLE>
    <CHASEKICK_TOPENALTY_GAP>10</CHASEKICK_TOPENALTY_GAP>
    <CHASEKICK_ENDVEL_STARTX>0</CHASEKICK_ENDVEL_STARTX>
    <CHASEKICK_MAXENDVEL>0</CHASEKICK_MAXENDVEL>
    <BALL_NEAR_ROBOT>20</BALL_NEAR_ROBOT>
    <USE_DRIBBLE_TURN>1</USE_DRIBBLE_TURN>
    <FLAT_DIST>600</FLAT_DIST>
    <PULL_DIST>450</PULL_DIST>
    <RELIEF_DIST>350</RELIEF_DIST>  <!-- double 可解围的范围 -->
    <KICKPOWER>650</KICKPOWER><!-- 射出的球速cm/s -->
    <CHIPPOWER>155</CHIPPOWER><!-- 挑球距离cm -->
    <WEAK_OPP>1</WEAK_OPP>
    <OPP_HAS_BALL_DIST>50</OPP_HAS_BALL_DIST>
    <CIRCLE_FRONT>1</CIRCLE_FRONT>
    <DEBUG_ENGINE>1</DEBUG_ENGINE>
<!--     <GETBALL_SPEED>100</GETBALL_SPEED>
    <GETBALL_ROT_SPEED>3</GETBALL_ROT_SPEED>
    <GETBALL_ACCELERATION>100</GETBALL_ACCELERATION>
    <GETBALL_DECELERATION>100</GETBALL_DECELERATION>
    <GETBALL_ROT_ACCELERATION>5</GETBALL_ROT_ACCELERATION>     -->
    <GETBALL_SPEED>100</GETBALL_SPEED>
    <GETBALL_ROT_SPEED>3</GETBALL_ROT_SPEED>
    <GETBALL_ACCELERATION>50</GETBALL_ACCELERATION>
    <GETBALL_DECELERATION>100</GETBALL_DECELERATION>
    <GETBALL_ROT_ACCELERATION>3</GETBALL_ROT_ACCELERATION>
    <CLOSE_TO_BALL_DISTANCE>1</CLOSE_TO_BALL_DISTANCE>
  </ADVANCE>
  <PENALTY_DEFENCE>
    <CATEGORY>3</CATEGORY>
    <FRAME>0</FRAME>
    <AHEAD>20</AHEAD>
  </PENALTY_DEFENCE>
  <TOUCHKICK>
      <TOUCH_SHIFT_DIST>0</TOUCH_SHIFT_DIST>
      <TOUCH_Debug>0</TOUCH_Debug>
  </TOUCHKICK>
  <ParamManagerNew>
      <PENALTY_AREA_DEPTH>100</PENALTY_AREA_DEPTH>
